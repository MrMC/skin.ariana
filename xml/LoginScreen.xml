<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">52</defaultcontrol>
  <controls>
    <control type="image" description="Art">
      <left>0</left>
      <top>0</top>
      <width>100%</width>
      <height>100%</height>
      <aspectratio>stretch</aspectratio>
      <texture colordiffuse="FF7f8da8">background/LightBG.png</texture>
    </control>
    <control type="group">
      <include>OpenClose</include>
      <control type="wraplist" id="52" description="users">
        <focusposition>2</focusposition>
        <left>60</left>
        <top>420</top>
        <width>1800</width>
        <height>408</height>
        <orientation>horizontal</orientation>
        <itemlayout height="408" width="360">
          <control type="image">
            <left>45</left>
            <top>45</top>
            <width>270</width>
            <height>270</height>
            <texture colordiffuse="$VAR[OverlayColor]">thumbs/Monogram.png</texture>
            <bordersize>-24</bordersize>
            <aspectratio>scale</aspectratio>
          </control>
          <control type="label" description="Monogram">
            <left>45</left>
            <top>45</top>
            <width>270</width>
            <height>270</height>
            <font>monogram</font>
            <align>center</align>
            <aligny>center</aligny>
            <textcolor>$VAR[Label2Color]</textcolor>
            <label>$INFO[ListItem.Property(monogram)]</label>
          </control>
          <control type="image">
            <left>45</left>
            <top>45</top>
            <width>270</width>
            <height>270</height>
            <texture diffuse="img/UserDiffuse.png">$INFO[ListItem.Thumb]</texture>
            <aspectratio scalediffuse="false">scale</aspectratio>
            <bordersize>-2</bordersize>
          </control>
        </itemlayout>
        <focusedlayout height="408" width="360">
          <control type="group">
            <animation type="Focus" reversible="false">
              <effect type="zoom" center="auto" start="100" end="133" time="120" easing="inout"/>
            </animation>
            <animation type="UnFocus" reversible="false">
              <effect type="zoom" center="auto" start="133" end="100" time="120" easing="inout"/>
            </animation>
            <control type="image">
              <left>45</left>
              <top>45</top>
              <width>270</width>
              <height>270</height>
              <texture colordiffuse="$VAR[OverlayColor]">thumbs/Monogram.png</texture>
              <bordersize>-24</bordersize>
              <aspectratio>scale</aspectratio>
            </control>
            <control type="label" description="Monogram">
              <left>45</left>
              <top>45</top>
              <width>270</width>
              <height>270</height>
              <font>monogram</font>
              <align>center</align>
              <aligny>center</aligny>
              <textcolor>$VAR[Label2Color]</textcolor>
              <label>$INFO[ListItem.Property(monogram)]</label>
            </control>
            <control type="image">
              <left>45</left>
              <top>45</top>
              <width>270</width>
              <height>270</height>
              <font>monogram</font>
              <texture diffuse="img/UserDiffuse.png">$INFO[ListItem.Thumb]</texture>
              <aspectratio scalediffuse="false">scale</aspectratio>
              <bordersize>-2</bordersize>
            </control>
            <control type="label" description="Main Label">
              <left>0</left>
              <right>0</right>
              <top>310</top>
              <height>66</height>
              <font>size19</font>
              <align>center</align>
              <textcolor>$VAR[LabelNFColor]</textcolor>
              <label>$INFO[ListItem.Label]</label>
            </control>
          </control>
        </focusedlayout>
      </control>
      <control type="group" description="52 furniture">
        <visible>Control.IsVisible(52)</visible>
        <control type="label" description="Main Label">
          <centerleft>50%</centerleft>
          <top>150</top>
          <align>center</align>
          <aligny>center</aligny>
          <width>100%</width>
          <height>60</height>
          <font>size50B</font>
          <label>$LOCALIZE[20096]</label>
        </control>
        <control type="label" description="INFO">
          <centerleft>50%</centerleft>
          <bottom>30</bottom>
          <height>60</height>
          <width>100%</width>
          <align>center</align>
          <font>size16</font>
          <textcolor>$VAR[Label2Color]</textcolor>
          <label>$INFO[Container(52).ListItem.Label2]</label>
        </control>
      </control>
    </control>
  </controls>
</window>
