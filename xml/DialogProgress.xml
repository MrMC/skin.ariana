        <height>50</height>
        <align>center</align>
        <font>size28B</font>
        <label>$INFO[Control.GetLabel(20),,%]</label>
      </control>
      <control type="progress" id="20" description="Progressbar">
        <top>-1556</top>
        <centerleft>960</centerleft>
