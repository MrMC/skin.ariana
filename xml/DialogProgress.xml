      <control type="label" id="1" description="Heading">
        <top>240</top>
        <centerleft>960</centerleft>
        <label>-</label>
        <height>32</height>
        <width>100%</width>
        <font>size50B</font>
        <align>center</align>
        <textcolor>$VAR[LabelNFColor]</textcolor>
      </control>
      <control type="label" id="2" description="Line 1">
        <top>340</top>
        <centerleft>960</centerleft>
        <width>960</width>
        <height>50</height>
        <align>center</align>
        <font>size20</font>
        <label>111111</label>
      </control>
      <control type="label" id="3" description="Line 2">
        <top>390</top>
        <centerleft>960</centerleft>
        <width>960</width>
        <height>50</height>
        <align>center</align>
        <font>size20</font>
        <label>222222</label>
      </control>
      <control type="label" id="4" description="Line 3">
        <top>440</top>
        <centerleft>960</centerleft>
        <width>960</width>
        <height>50</height>
        <align>center</align>
        <font>size20</font>
        <label>333333</label>
      </control>
      <control type="label" id="21" description="Progress">
        <top>580</top>
        <centerleft>960</centerleft>
